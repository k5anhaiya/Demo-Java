// ABCDEDCBA
// ABCD DCBA
// ABC   CBA
// AB     BA
// A       A
public class abcdedcba 
{
	public static void main(String[] args) 
	{
		int c=65;
		for(int r=1;r<=5;r++)
		{
			for(int i=65;i<=70-r;i++)
			{
			System.out.print((char)i);
			}
			for(int i=70-r;i<=65;i--)
			{
				if(c!=69)
			System.out.print((char)i);
			}
			
			System.out.println();
		}
	}

}
