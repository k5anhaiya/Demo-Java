
public class App1 
{

	public static void main(String[] args) 
	{
		int [] num= {50,33,80,12,345,67,190,1,45,66,120};
		  //Write code to display total of even and total of odd
		  int even=0,odd=0;
		  for(int x:num)
		  {
		   if(x%2==0)
		    even++;
		   else
		    odd++;
		  }
		  System.out.println("Totan even number are "+even);
		  System.out.println("Totan odd number are "+odd);
	}

}
